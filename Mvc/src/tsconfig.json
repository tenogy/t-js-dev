	"compilerOptions": {
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": true,
		"sourceMap": false,
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "classic",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"pretty": true,
		"outDir": "./../.build/"
	},
	"compileOnSave": false
